{% extends 'base.html.twig' %}

{% block title %}CityHead index{% endblock %}

{% block body %}
    <h1>CityHead index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for city_head in city_heads %}
            <tr>
                <td>{{ city_head.id }}</td>
                <td>
                    <a href="{{ path('city_head_show', {'id': city_head.id}) }}">show</a>
                    <a href="{{ path('city_head_edit', {'id': city_head.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('city_head_new') }}">Create new</a>
{% endblock %}
