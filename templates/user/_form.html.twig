
<div class="container">
{{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.password) }}
    {{ form_row(form.phone) }}
    {{ form_row(form.roles) }}
    {{ form_row(form.city) }}

    <div class="col s3" id="recipient-concession">
    </div>
    <div class="col s3" id="recipient-service">
    </div>
    <div class="row with-hidden-fields">
            {{form_widget(form.service)}}
    </div>
    <button class="btn light-blue darken-2">{{ button_label|default('Ajouter') }}</button>
{{ form_end(form) }}
</div>
